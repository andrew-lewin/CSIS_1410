package pacman;

import javax.swing.ImageIcon;


public class Pacman {
	private ImageIcon pac;
	private static int location;
	
	public Pacman(){
		pac = new ImageIcon("pacmanStart.png");
		location = 149;
	}
	
	public void setLocation(int direction){
		location = direction;
	}
	
	public static int getLocation(){
		return location;
	}
	
	public ImageIcon getImage() {
		return pac;
	}

	public void setImage(ImageIcon pac) {
		this.pac = pac;
	}

}
